{
  "name": "api-nfl-schedule",
  "version": "1.0.0",
  "description": "An espn NFL schedule scraping api.",
  "engines": {
    "node": ">=4.1.1"
  },
  "repository": {
    "git": "git@github.com:n8io/api-nfl-schedule.git"
  },
  "main": "./app/app.js",
  "scripts": {
    "start": "node ./app/app.js",
    "test": "gulp test",
    "dev": "gulp | bunyan",
    "gulp": "gulp",
    "clean": "rm -rf ./node_modules/ ./dist/ ./.git.json ./coverage ./*.log 2 ./Dockerfile ./.dockerignore > /dev/null",
    "docker-build": "cp -rf ./docker/build/ ./ && docker build --tag api-nfl-schedule:latest . && docker-compose stop && docker-compose rm -f && docker-compose up -d",
    "docker-local": "cp -rf ./docker/local/ ./ && docker-compose stop && docker-compose rm -f && docker-compose up -d"
  },
  "author": "Nate Clark",
  "license": "MIT",
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "bunyan-loggly": "0.0.5",
    "cheerio": "^0.19.0",
    "express": "^4.13.3",
    "express-redis-cache": "^0.1.8",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "redis": "^2.2.1",
    "request": "^2.64.0",
    "require-dir": "^0.3.0",
    "serve-favicon": "^2.3.0",
    "sort-json": "^1.1.1"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "chai": "^3.3.0",
    "gulp": "^3.9.0",
    "gulp-debug": "^2.1.2",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-mocha": "^2.1.3",
    "gulp-nodemon": "^2.0.4",
    "gulp-sequence": "^0.4.1",
    "http-status": "^0.2.0",
    "mocha": "^2.3.3",
    "shelljs": "^0.5.3",
    "superagent": "^1.4.0"
  }
}
